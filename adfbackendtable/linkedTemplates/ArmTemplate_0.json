			"type": "string",
			"defaultValue": "DESKTOP-38EI4G6\\SQLEXPRESS"
		},
		"DS_SqlServerAdventureWorks_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorks2019"
		},
		"DS_SqlServerAdventureWorks_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "janaki"
		},
		"LS_Adventureworks_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "DESKTOP-38EI4G6\\SQLEXPRESS"
		},
		"LS_Adventureworks_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorks2019"
		},
		"LS_Adventureworks_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "janaki"
		},
		"LS_MyDataBase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "DESKTOP-38EI4G6\\SQLEXPRESS"
		},
		"LS_MyDataBase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "MyDataBase"
		},
		"LS_MyDataBase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "janaki"
